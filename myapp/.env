# Give your application a unique name (no spaces)
APP_NAME=myapp

# false for production, true for development
DEBUG=true

# the port to listen on
PORT=4000

# the server name, e.g: www.mysite.com
SERVER_NAME=localhost

# should we use https?
SECURE=false

# database config - postgres or mysql
DATABASE_TYPE=postgres
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASS=password
DATABASE_NAME=goravel
DATABASE_SSL_MODE=disable

# redis config
REDIS_HOST="localhost:6379"
REDIS_PASSWORD=
REDIS_PREFIX=goravel

# cache (currently only support redis)
CACHE=redis

# cookie settings
COOKIE_NAME=goravel
COOKIE_LIFETIME=1440
COOKIE_PERSIST=true
COOKIE_SECURE=false
COOKIE_DOMAIN=localhost

# sessions store: cookie, redis, mysql or postgres
SESSION_TYPE=redis

